{
  "include": [
    "**/*",
    "**/.server/**/*",
    "**/.client/**/*",
    ".react-router/types/**/*"
  ],
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "types": [
      "node",
      "vite/client"
    ],
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@components/*": [
        "./app/components/*"
      ],
      "@types/*": [
        "./app/types/*"
      ],
      "@hooks/*": [
        "./app/hooks/*"
      ],
      "@utils/*": [
        "./app/utils/*"
      ],
      "@context/*": [
        "./app/context/*"
      ],
      "@stores/*": [
        "./app/stores/*"
      ],
      "@services/*": [
        "./app/services/*"
      ],
      "@lib/*": [
        "./app/lib/*"
      ]
    },
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  }
}